:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x5a3ac053a5c0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5a3ac053a750 .scope module, "sync_fifo_TB2" "sync_fifo_TB2" 3 1;
 .timescale 0 0;
v0x5a3ac0560370_0 .var "clk", 0 0;
v0x5a3ac0560430_0 .var "data_in", 7 0;
v0x5a3ac05604d0_0 .net "data_out", 7 0, v0x5a3ac055fb00_0;  1 drivers
v0x5a3ac05605d0_0 .net "empty", 0 0, L_0x5a3ac0560c70;  1 drivers
v0x5a3ac05606a0_0 .net "full", 0 0, L_0x5a3ac0560b30;  1 drivers
v0x5a3ac0560740_0 .var "r_en", 0 0;
v0x5a3ac0560810_0 .var "rst_n", 0 0;
v0x5a3ac05608e0_0 .var "w_en", 0 0;
S_0x5a3ac0530fe0 .scope task, "drive" "drive" 3 32, 3 32 0, S_0x5a3ac053a750;
 .timescale 0 0;
v0x5a3ac052e7e0_0 .var/2s "delay", 31 0;
E_0x5a3ac0540c90 .event posedge, v0x5a3ac052abf0_0;
TD_sync_fifo_TB2.drive ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac05608e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac0560740_0, 0, 1;
    %fork t_1, S_0x5a3ac0530fe0;
    %fork t_2, S_0x5a3ac0530fe0;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %pushi/vec4 10, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5a3ac0540c90;
    %fork TD_sync_fifo_TB2.push, S_0x5a3ac055f480;
    %join;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac05608e0_0, 0, 1;
    %end;
t_2 ;
    %load/vec4 v0x5a3ac052e7e0_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %pushi/vec4 10, 0, 32;
T_0.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.3, 5;
    %jmp/1 T_0.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5a3ac0540c90;
    %fork TD_sync_fifo_TB2.pop, S_0x5a3ac055f280;
    %join;
    %jmp T_0.2;
T_0.3 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac0560740_0, 0, 1;
    %end;
    .scope S_0x5a3ac0530fe0;
t_0 ;
    %end;
S_0x5a3ac055f280 .scope task, "pop" "pop" 3 25, 3 25 0, S_0x5a3ac053a750;
 .timescale 0 0;
TD_sync_fifo_TB2.pop ;
    %load/vec4 v0x5a3ac05605d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a3ac0560740_0, 0, 1;
    %delay 1, 0;
    %vpi_call/w 3 28 "$display", "Pop Out: w_en=%b, r_en=%b, data_out=%h", v0x5a3ac05608e0_0, v0x5a3ac0560740_0, v0x5a3ac05604d0_0 {0 0 0};
    %jmp T_1.5;
T_1.4 ;
    %vpi_call/w 3 30 "$display", "FIFO Empty!! Can not pop data_out" {0 0 0};
T_1.5 ;
    %end;
S_0x5a3ac055f480 .scope task, "push" "push" 3 17, 3 17 0, S_0x5a3ac053a750;
 .timescale 0 0;
TD_sync_fifo_TB2.push ;
    %load/vec4 v0x5a3ac05606a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.6, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a3ac05608e0_0, 0, 1;
    %vpi_func 3 20 "$random" 32 {0 0 0};
    %pad/s 8;
    %store/vec4 v0x5a3ac0560430_0, 0, 8;
    %delay 1, 0;
    %vpi_call/w 3 21 "$display", "Push In: w_en=%b, r_en=%b, data_in=%h", v0x5a3ac05608e0_0, v0x5a3ac0560740_0, v0x5a3ac0560430_0 {0 0 0};
    %jmp T_2.7;
T_2.6 ;
    %vpi_call/w 3 23 "$display", "FIFO Full!! Can not push data_in=%d", v0x5a3ac0560430_0 {0 0 0};
T_2.7 ;
    %end;
S_0x5a3ac055f660 .scope module, "s_fifo" "synchronous_fifo" 3 7, 4 1 0, S_0x5a3ac053a750;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "w_en";
    .port_info 3 /INPUT 1 "r_en";
    .port_info 4 /INPUT 8 "data_in";
    .port_info 5 /OUTPUT 8 "data_out";
    .port_info 6 /OUTPUT 1 "full";
    .port_info 7 /OUTPUT 1 "empty";
P_0x5a3ac0528230 .param/l "DATA_WIDTH" 0 4 1, +C4<00000000000000000000000000001000>;
P_0x5a3ac0528270 .param/l "DEPTH" 0 4 1, +C4<00000000000000000000000000001000>;
L_0x748b424d6018 .functor BUFT 1, C4<001>, C4<0>, C4<0>, C4<0>;
v0x5a3ac052efb0_0 .net/2u *"_ivl_0", 2 0, L_0x748b424d6018;  1 drivers
v0x5a3ac052f6f0_0 .net *"_ivl_2", 2 0, L_0x5a3ac05609e0;  1 drivers
v0x5a3ac052abf0_0 .net "clk", 0 0, v0x5a3ac0560370_0;  1 drivers
v0x5a3ac055fa20_0 .net "data_in", 7 0, v0x5a3ac0560430_0;  1 drivers
v0x5a3ac055fb00_0 .var "data_out", 7 0;
v0x5a3ac055fc30_0 .net "empty", 0 0, L_0x5a3ac0560c70;  alias, 1 drivers
v0x5a3ac055fcf0 .array "fifo", 7 0, 7 0;
v0x5a3ac055fdb0_0 .net "full", 0 0, L_0x5a3ac0560b30;  alias, 1 drivers
v0x5a3ac055fe70_0 .net "r_en", 0 0, v0x5a3ac0560740_0;  1 drivers
v0x5a3ac055ff30_0 .var "r_ptr", 2 0;
v0x5a3ac0560010_0 .net "rst_n", 0 0, v0x5a3ac0560810_0;  1 drivers
v0x5a3ac05600d0_0 .net "w_en", 0 0, v0x5a3ac05608e0_0;  1 drivers
v0x5a3ac0560190_0 .var "w_ptr", 2 0;
L_0x5a3ac05609e0 .arith/sum 3, v0x5a3ac0560190_0, L_0x748b424d6018;
L_0x5a3ac0560b30 .cmp/eq 3, L_0x5a3ac05609e0, v0x5a3ac055ff30_0;
L_0x5a3ac0560c70 .cmp/eq 3, v0x5a3ac0560190_0, v0x5a3ac055ff30_0;
    .scope S_0x5a3ac055f660;
T_3 ;
    %wait E_0x5a3ac0540c90;
    %load/vec4 v0x5a3ac0560010_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5a3ac0560190_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5a3ac055ff30_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5a3ac055fb00_0, 0;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5a3ac055f660;
T_4 ;
    %wait E_0x5a3ac0540c90;
    %load/vec4 v0x5a3ac05600d0_0;
    %load/vec4 v0x5a3ac055fdb0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %load/vec4 v0x5a3ac055fa20_0;
    %load/vec4 v0x5a3ac0560190_0;
    %pad/u 5;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5a3ac055fcf0, 0, 4;
    %load/vec4 v0x5a3ac0560190_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x5a3ac0560190_0, 0;
T_4.0 ;
    %jmp T_4;
    .thread T_4;
    .scope S_0x5a3ac055f660;
T_5 ;
    %wait E_0x5a3ac0540c90;
    %load/vec4 v0x5a3ac055fe70_0;
    %load/vec4 v0x5a3ac055fc30_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %load/vec4 v0x5a3ac055ff30_0;
    %pad/u 5;
    %ix/vec4 4;
    %load/vec4a v0x5a3ac055fcf0, 4;
    %assign/vec4 v0x5a3ac055fb00_0, 0;
    %load/vec4 v0x5a3ac055ff30_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x5a3ac055ff30_0, 0;
T_5.0 ;
    %jmp T_5;
    .thread T_5;
    .scope S_0x5a3ac053a750;
T_6 ;
    %delay 2, 0;
    %load/vec4 v0x5a3ac0560370_0;
    %inv;
    %store/vec4 v0x5a3ac0560370_0, 0, 1;
    %jmp T_6;
    .thread T_6;
    .scope S_0x5a3ac053a750;
T_7 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac0560370_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac0560810_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac05608e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a3ac0560740_0, 0, 1;
    %delay 3, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a3ac0560810_0, 0, 1;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x5a3ac052e7e0_0, 0, 32;
    %fork TD_sync_fifo_TB2.drive, S_0x5a3ac0530fe0;
    %join;
    %pushi/vec4 40, 0, 32;
    %store/vec4 v0x5a3ac052e7e0_0, 0, 32;
    %fork TD_sync_fifo_TB2.drive, S_0x5a3ac0530fe0;
    %join;
    %vpi_call/w 3 15 "$finish" {0 0 0};
    %end;
    .thread T_7;
    .scope S_0x5a3ac053a750;
T_8 ;
    %vpi_call/w 3 47 "$dumpfile", "waveform2.vcd" {0 0 0};
    %vpi_call/w 3 47 "$dumpvars" {0 0 0};
    %end;
    .thread T_8;
